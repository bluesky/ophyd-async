}

record(waveform, "$(device)longstr") {
  field(NELM, "80")
  field(FTVL, "CHAR")
  field(INP, {const:"a string that is just longer than forty characters"})
  field(PINI, "YES")
}

record(lsi, "$(device)longstr2") {
  field(SIZV, "80")
  field(INP, {const:"a string that is just longer than forty characters"})
  field(PINI, "YES")
}
